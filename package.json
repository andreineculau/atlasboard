    "name":           "atlasboard",
    "description":    "AtlasBoard is dashboard/wallboard framework written all in JS",
    "version":        "0.1.1",
    "homepage":       "https://bitbucket.org/atlassian/atlasboard",
    "repository":     {
        "type":"git",
        "url": "git@bitbucket.org:atlassian/atlasboard.git"
    },
    "engines":        {
        "node":"~0.6.10"
    },
    "bin":            {
        "atlasboard":"lib/cli/commands.js"
    },
    "main":           "./lib/atlasboard.js",
    "dependencies":   {
        "express":              "3.0.0",
        "path":                 "0.4.9",
        "temp":                 "0.4.0",
        "underscore":           "1.4.2",
        "socket.io":            "0.9.13",
        "stylus":               "0.31.0",
        "nib":                  "0.9.0",
        "connect-assets":       "2.3.3",
        "ejs":                  "0.8.3",
        "async":                "0.2.6",
        "optimist":             "~0.3.5",
        "hardhat":              "~0.0.7",
        "css-parse":            "1.0.4",
        "cheerio":              "0.10.7",
        "ical":                 "0.0.6",
        "tracer":               "0.5.1",
        "request":              "2.14.0",
        "xtend":                "2.0.3"
    },
    "devDependencies":{
        "mocha":                "1.8.1",
        "expect.js":            "0.1.2",
        "rimraf":               "2.1.4"
    },
    "preferGlobal":   true,
    "keywords":       ["server", "dashboard"],
    "scripts":        {
        "test": "mocha -b $(find . -name *.mocha) --reporter list"
    }
}
