<html class="no-js" lang="en"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>AtlasBoard</title>
    <meta name="viewport" content="width=device-width">

    <link rel="stylesheet" href="/third-party/rickshaw/rickshaw.min.css" type="text/css" media="screen" />
    <link rel="stylesheet" href="/third-party/ducksboard/jquery.gridster.css" type="text/css" media="screen" />

    <link rel="stylesheet" href="/stylesheets/normalize.css" type="text/css" media="screen" />
    <link rel="stylesheet" href="/stylesheets/helpers.css" type="text/css" media="screen" />
    <link rel="stylesheet" href="/stylesheets/application.css" type="text/css" media="screen" />
    
    <script src="/third-party/jquery/jquery-1.8.3.min.js"></script>
    <script src="/third-party/underscore/underscore-1.4.4.min.js"></script>
    <script src="/third-party/ducksboard/jquery.gridster-0.1.0.min.js"></script>
    <!--<script src="/third-party/d3/d3.min.js"></script>-->
    <script src="/third-party/d3/d3.js"></script>
    <!--<script src="/third-party/rickshaw/rickshaw.min.js"></script>-->
    <script src="/third-party/rickshaw/rickshaw.js"></script>
    <script src="/socket.io/socket.io.js"></script>

    <script src="/javascripts/console-helper.js"></script>
    <script src="/javascripts/application.js"></script>
    <script src="/<%=dashboardName%>/js"></script>

    <link rel="icon" type="image/png" href="images/favicon.png"/>
</head>
<body>
<div id="disconnected">DISCONNECTED</div>
<% if (dashboardConfig.title) { %>
    <h1><%= dashboardConfig.title %></h1>
<% } %>
<div id="main-container">
    <div id="widgets-container" class="gridster">
        <ul>
            <% dashboardConfig.layout.widgets.forEach(function (widget) { %>
                <li data-row="<%= widget.row %>" data-col="<%= widget.col %>" data-sizex="<%= widget.width %>"
                    data-sizey="<%= widget.height %>" data-widget-id="<%= widget.widget %>"
                    data-event-id="<%=(dashboardName + "_" + widget.config + "_" + widget.widget + "_" +  widget.job) %>"></li>
            <% }) %>
        </ul>
    </div>
</div>
</body>
</html>
